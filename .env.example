# Voting window in minutes (how long to wait for votes)
COMMUNITY_VOTING_WINDOW_MINUTES=30

# Minimum votes required to count (if less, defaults to approve)
# Setting this low ensures apathy != rejection
COMMUNITY_VOTING_MIN_VOTES=3

