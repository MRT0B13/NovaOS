# ============================================================================
# ğŸš€ LAUNCHKIT - AUTONOMOUS TOKEN LAUNCHER
# ============================================================================
# Copy this file to .env and fill in your actual values
#
# QUICK START:
# 1. Add your Anthropic API key (REQUIRED)
# 2. Add your OpenAI API key (REQUIRED for embeddings)
# 3. Add Solana RPC URL (REQUIRED)
# 4. Add Pump Portal credentials (REQUIRED)
# 5. Add wallet secrets (REQUIRED)
# 6. Optional: Add Telegram bot token
# 7. Optional: Add Twitter API credentials
# 8. Run: bun start
# ============================================================================

# ============================================================================
# âš¡ REQUIRED: AI MODEL PROVIDERS
# ============================================================================

# Anthropic API Key (Claude 3.5 Sonnet) - PRIMARY MODEL
# Get from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI API Key (needed for embeddings even if using Claude)
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# ============================================================================
# âš¡ REQUIRED: SOLANA & PUMP.FUN CONFIGURATION
# ============================================================================

# Solana RPC URL (Mainnet) - CRITICAL FOR TOKEN LAUNCHES
# Free tier: https://www.alchemy.com/ or https://www.helius.dev/
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Pump Portal API Configuration
# Sign up: https://pumpportal.fun/
PUMP_PORTAL_API_KEY=your_pump_portal_api_key_here

# Pump Portal Wallet (dedicated wallet for pump.fun - needs ~0.3-0.5 SOL per launch)
# Generate a new Solana keypair for this
PUMP_PORTAL_WALLET_SECRET=your_pump_wallet_private_key_base58
PUMP_PORTAL_WALLET_ADDRESS=your_pump_wallet_public_address

# Agent Funding Wallet (your main SOL wallet - transfers SOL to pump wallet)
# Use your existing Phantom/Solflare wallet private key
AGENT_FUNDING_WALLET_SECRET=your_main_wallet_private_key_base58

# ============================================================================
# âš¡ REQUIRED: LAUNCHKIT CONFIGURATION
# ============================================================================

LAUNCHKIT_ENABLE=true
LAUNCH_ENABLE=true
LAUNCHKIT_PORT=8787

# Admin tokens (generate random secure tokens)
LAUNCHKIT_ADMIN_TOKEN=generate_a_random_secure_token_here
ADMIN_TOKEN=generate_another_random_secure_token_here

# Launch Safety Limits
MAX_SOL_DEV_BUY=0.1
MAX_PRIORITY_FEE=0.0005
MAX_LAUNCHES_PER_DAY=3
LAUNCH_SLIPPAGE_PERCENT=5
MAX_SLIPPAGE_PERCENT=5

# Wallet Features
LOCAL_WITHDRAW_ENABLE=true

# ============================================================================
# ğŸ“± OPTIONAL: TELEGRAM BOT (Recommended)
# ============================================================================
# Enables community management, group linking, auto-announcements

TG_ENABLE=true

# Telegram Bot Token
# Get from: @BotFather on Telegram
# Instructions: /newbot -> follow prompts -> copy token
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# ============================================================================
# ğŸ¦ OPTIONAL: TWITTER/X INTEGRATION (Recommended)
# ============================================================================
# Auto-posts launch announcements and marketing threads

X_ENABLE=true

# Twitter API Credentials (OAuth 1.0a)
# Get from: https://developer.twitter.com/en/portal/dashboard
# Steps:
#   1. Create a Twitter App
#   2. Enable OAuth 1.0a with Read & Write permissions
#   3. Generate API Key & Secret (Consumer Keys)
#   4. Generate Access Token & Secret
TWITTER_API_KEY=your_twitter_api_key_here
TWITTER_API_SECRET_KEY=your_twitter_api_secret_here
TWITTER_ACCESS_TOKEN=your_twitter_access_token_here
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret_here

# Twitter Automation Settings
TWITTER_DRY_RUN=false
TWITTER_ENABLE_POSTING=true

# X Scheduler Configuration
# Tweets per day per launched token (conservative for rate limits)
X_AUTO_TWEETS_PER_DAY=2
# Auto-refill marketing queue when pending tweets drops below this
X_MIN_PENDING_TWEETS=5
# How many days ahead to schedule tweets
X_REFILL_DAYS=3
# How often to post Nova channel promos (in days)
X_CHANNEL_PROMO_INTERVAL_DAYS=1
# Keep this many channel promos scheduled ahead
X_MIN_PENDING_CHANNEL_PROMOS=7

# ============================================================================
# ğŸ“¢ OPTIONAL: NOVA CHANNEL (Agent's Own TG Channel)
# ============================================================================
# Nova can manage its own Telegram channel for announcements and community.
# Great for transparency - lets your community see all Nova's activity!

# Enable Nova's personal channel
NOVA_CHANNEL_ENABLE=false

# Telegram channel/group ID for Nova's announcements
# Get this by adding @RawDataBot to your channel, or use @getidsbot
NOVA_CHANNEL_ID=

# Public invite link for Nova's channel (for X marketing tweets)
# This is the t.me link that will be shared in tweets to grow the channel
NOVA_CHANNEL_INVITE=

# Nova's X (Twitter) handle (without @) for autonomous launches
# When set, autonomous launches will link to this X account
NOVA_X_HANDLE=

# Types of updates to post (comma-separated)
# Options: launches, wallet, health, marketing, system
# - launches: New token launch announcements
# - wallet: Withdrawal/deposit notifications
# - health: Hourly community health summaries
# - marketing: When marketing posts go out
# - system: Startup/shutdown notifications
NOVA_CHANNEL_UPDATES=launches,wallet,health

# ============================================================================
# ğŸ“Š OPTIONAL: SYSTEM REPORTS (Stay Informed When AFK)
# ============================================================================
# Sends periodic status reports to your admin chat so you always know
# what Nova is up to, even when you're away.

# Enable automatic system reports
# - Status report every 4 hours
# - Daily summary at 9 AM UTC
# You can also ask "status report" in chat anytime
SYSTEM_REPORTS_ENABLE=false

# ============================================================================
# ğŸ”’ OPTIONAL: TELEGRAM SECURITY
# ============================================================================
# Security settings for Telegram bot commands

# Comma-separated list of Telegram user IDs allowed to run admin commands
# Get your ID from @userinfobot on Telegram
# TELEGRAM_ADMIN_IDS=123456789,987654321

# Secret token for webhook verification (set when configuring webhook)
# Generate with: openssl rand -hex 32
# TG_WEBHOOK_SECRET=your_webhook_secret_here

# Disable auto-restart of Telegram polling
# Set to true if using webhooks (polling restart doesn't apply)
TG_DISABLE_AUTO_RESTART=false

# ============================================================================
# ğŸ¨ OPTIONAL: AI IMAGE GENERATION
# ============================================================================
# Uses DALL-E 3 via OpenAI API

# Enable AI-generated logos for token launches
AI_LOGO_ENABLE=true

# Enable AI-generated memes for Telegram shills
AI_MEME_ENABLE=true

# ============================================================================
# ğŸ–¥ï¸  ELIZA UI & DATABASE
# ============================================================================

# ElizaOS Web Dashboard
# Access at: http://localhost:3000 when running
ELIZA_UI_ENABLE=true
ELIZA_DISABLE_SERVER=false
ELIZA_SERVER_URL=

# Database - PGlite (embedded SQL database, no setup needed)
PGLITE_DATA_DIR=./data/pglite
PGLITE_PATH=./.pglite/launchkit

# Optional: Use PostgreSQL instead of PGlite
# Uncomment and configure if you have a Postgres instance:
# DATABASE_URL=postgresql://user:password@host:5432/database

# ============================================================================
# â˜ï¸  RAILWAY DEPLOYMENT CONFIGURATION
# ============================================================================
# When deploying to Railway, set these variables in the Railway dashboard.
# Railway auto-injects DATABASE_URL and PORT.
#
# Required Railway Variables:
#   DATABASE_URL    - Auto-injected by Railway Postgres plugin
#   PORT            - Auto-injected by Railway (we bind to 0.0.0.0:$PORT)
#
# Recommended Railway Variables:
#   LAUNCHKIT_ENABLE=true
#   LAUNCH_ENABLE=true
#   ADMIN_TOKEN=<generate-secure-random-token>
#   LAUNCHKIT_ADMIN_TOKEN=<generate-secure-random-token>
#
# Optional: Disable vector embeddings if pgvector not available
#   SQL_EMBEDDINGS_ENABLE=false

# Railway Postgres Notes:
# - Railway Postgres 17 may NOT have pgvector extension
# - We gracefully detect this and disable vector storage
# - central_messages schema is auto-created on startup
# - No manual SSH/psql needed

# ============================================================================
# ğŸ”§ OPTIONAL: DISCORD INTEGRATION
# ============================================================================

DISCORD_APPLICATION_ID=
DISCORD_API_TOKEN=

# ============================================================================
# ğŸ¦ TREASURY WALLET & PROFIT MANAGEMENT (Advanced)
# ============================================================================
# Configure a separate treasury wallet for storing profits long-term.
# Treasury is address-only (no private key stored) for security.
#
# IMPORTANT: All features below are DISABLED by default for safety.
# Enable only after you understand the implications.

# Enable treasury wallet integration
# When enabled, withdrawals go to TREASURY_ADDRESS instead of funding wallet
TREASURY_ENABLE=false

# Treasury wallet address (public key only - NEVER add private key!)
# This is your cold storage or hardware wallet for profits
TREASURY_ADDRESS=

# Minimum SOL to leave in pump wallet as reserve for future launches
TREASURY_MIN_RESERVE_SOL=0.3

# Log-only mode: compute transfers without executing (safe preview mode)
# Set to false to enable actual treasury transfers
TREASURY_LOG_ONLY=true

# ============================================================================
# ğŸ”„ AUTO-WITHDRAW / SWEEP CONFIGURATION (Advanced)
# ============================================================================
# Automatically sweep profits from pump wallet when thresholds are met.
# Scheduler-driven, deterministic (not LLM-driven).

# Enable automatic threshold-based sweeps
AUTO_WITHDRAW_ENABLE=false

# Only withdraw when pump wallet balance exceeds this amount
WITHDRAW_MIN_SOL=0.25

# Keep this amount in pump wallet as runway for fees/launches
WITHDRAW_KEEP_SOL=0.15

# Maximum SOL that can be withdrawn per day (safety cap)
WITHDRAW_MAX_SOL_PER_DAY=2

# ============================================================================
# ğŸ“ˆ AUTO-SELL POLICY (Advanced - USE WITH CAUTION)
# ============================================================================
# Automatic take-profit selling for launched tokens.
# DISABLED by default - no autonomous trading without explicit enablement.
#
# Modes:
#   off           - No auto-selling (default)
#   manual_approve - Creates intents, requires approval (recommended first)
#   autonomous    - Fully automatic (use with extreme caution)

# Enable auto-sell evaluation
AUTO_SELL_ENABLE=false

# Auto-sell mode: off | manual_approve | autonomous
AUTO_SELL_MODE=off

# Cooldown between sells in seconds (with jitter for unpredictability)
AUTO_SELL_COOLDOWN_SECONDS=300

# Maximum percentage of holdings to sell per transaction
AUTO_SELL_MAX_PERCENT_PER_TX=20

# Max sell transactions per hour (rate limit)
AUTO_SELL_MAX_TX_PER_HOUR=10

# Custom take-profit policy (JSON string, optional)
# If not set, uses default ladder: 20% at 2x, 20% at 4x, 20% at 8x
# Example: {"take_profit":[{"at_x":2,"sell":25},{"at_x":5,"sell":25}],"moonbag":20}
# AUTO_SELL_POLICY_JSON=

